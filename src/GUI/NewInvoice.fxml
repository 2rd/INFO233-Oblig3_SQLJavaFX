<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane fx:id="parent" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.NewInvoiceController">
   <children>
      <VBox>
         <children>
            <VBox  maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="82.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="invoiceId" promptText="invoice id" />
                  <TextField fx:id="customerId" promptText="customer id" />
                  <TextField fx:id="date" promptText="date" />
               </children>
            </VBox>
            <VBox prefHeight="199.0" prefWidth="600.0" fx:id="products">
               <children>
                  <TextField fx:id="prod1" promptText="produkt 1" />
               </children>
            </VBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                   <Button fx:id="home" mnemonicParsing="false" onAction="#homeBut_click" text="Hjem">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#newProd_click" text="Legg til ekstra produkt">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="enter" mnemonicParsing="false" onAction="#button_addInvoice" text="Opprett faktura">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
