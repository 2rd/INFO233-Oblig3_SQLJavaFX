                          <TextField fx:id="streetNr" promptText="Husnummer" />
                          <TextField fx:id="postCode" promptText="Postnummer" />
                          <TextField fx:id="postTown" promptText="Poststed" />
                      </children>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <HBox>
               <children>
                    <Button fx:id="home" mnemonicParsing="false" onAction="#homeBut_click" text="Hjem">
                     <HBox.margin>
                        <Insets bottom="20.0" left="20.0" />
                     </HBox.margin>
                  </Button>
                    <Button fx:id="enter" mnemonicParsing="false" onAction="#butAddCust_click" text="Opprett kunde">
