                      <children>
                          <TextField fx:id="customerId" promptText="Kunde-ID" />
                          <TextField fx:id="customerName" promptText="Kundenavn" />
                          <TextField fx:id="phoneNr" promptText="Telefon" />
                          <TextField fx:id="account" promptText="FakturakontoNr" />
                          <TextField fx:id="addressId" promptText="adresseId" />
